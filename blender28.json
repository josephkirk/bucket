{
    "version": "2.8b",
    "license": "GPL-3.0-or-later",
    "checkver": {
        "url": "https://builder.blender.org/download/",
        "re": "blender-(?<version>(?<major>[\\d]+).(?<minor>[\\d]+))-(?<build>(?:[a-z]+)?)-windows64.zip"
    },
    "architecture": {
        "64bit": {
            "url": "https://builder.blender.org/download/blender-2.80-6d89337257b6-win64.zip",
            "extract_dir": "blender-2.80-git.6d89337257b6-windows64"
        },
        "32bit": {
            "url": "https://builder.blender.org/download/blender-2.80-6d89337257b6-win32.zip",
            "extract_dir": "blender-2.80.0-git.6d89337257b6-windows32"
        }
    },
    "homepage": "https://www.blender.org/",
    "bin": [
        ["blender.exe", "blender28"],
        ["blenderplayer.exe", "blender28player"]
    ],
    "shortcuts": [
        [
            "blender.exe",
            "Blender28"
        ]
    ],

    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://builder.blender.org/download/blender-$version-$build-windows64.zip",
                "extract_dir": "blender-$version.0-git.$version-windows64"
            },
            "32bit": {
                "url": "https://builder.blender.org/download/blender-$version-$build-windows32.zip",
                "extract_dir": "blender-$version.0-git.$version-windows32"
            }
        }
    }
}
